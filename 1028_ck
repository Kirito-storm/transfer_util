ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `new_sku_inv_amt_similar` Decimal(38,4) COMMENT '新品库存金额-同类值,同类店铺的(周期内总销售数量)';
-- 销存差-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `sales_inventory_diff_similar` Decimal(38,4) COMMENT '销存差-同类值,同类店铺的(周期内总销售吊牌额)';
-- 鞋类新品销售占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `shoe_new_sku_sales_amt_actual_pct_similar` Decimal(38,4) COMMENT '鞋类新品销售占比-同类值,同类店铺的(鞋类新品销售额÷鞋类销售额)';
-- 鞋类新品库存占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `shoe_new_sku_inv_amt_pct_similar` Decimal(38,4) COMMENT '鞋类新品库存占比-同类值,同类店铺的(鞋类新品库存金额÷鞋类库存金额)';
-- 鞋类销存差-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `shoe_sales_inventory_diff_similar` Decimal(38,4) COMMENT '鞋类销存差-同类值,同类店铺的(鞋类新品销售占比-鞋类新品库存占比)';
-- 服装新品销售占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `clothe_new_sku_sales_amt_actual_pct_similar` Decimal(38,4) COMMENT '服装新品销售占比-同类值,同类店铺的(服装新品销售额÷服装销售额)';
-- 服装新品库存占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `clothe_new_sku_inv_amt_pct_similar` Decimal(38,4) COMMENT '服装新品库存占比-同类值,同类店铺的(服装新品库存金额÷服装库存金额)';
-- 服装销存差-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `clothe_sales_inventory_diff_similar` Decimal(38,4) COMMENT '服装销存差-同类值,同类店铺的(服装新品销售占比-服装新品库存占比)';
-- 配件新品销售占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `part_new_sku_sales_amt_actual_pct_similar` Decimal(38,4) COMMENT '配件新品销售占比-同类值,同类店铺的(配件新品销售额÷配件销售额)';
-- 配件新品库存占比-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `part_new_sku_inv_amt_pct_similar` Decimal(38,4) COMMENT '配件新品库存占比-同类值,同类店铺的(配件新品库存金额÷配件库存金额)';
-- 配件销存差-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `part_sales_inventory_diff_similar` Decimal(38,4) COMMENT '配件销存差-同类值,同类店铺的(配件新品销售占比-配件新品库存占比)';
-- 库存款数-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `inv_sku_cnt_similar` int COMMENT '库存款数-同类值,同类店铺的(有库存的SKU数)';
-- 鞋类30天不动销款数-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `shoe_stay_sku_cnt_similar` int COMMENT '鞋类30天不动销款数-同类值,同类店铺的(到店天数>30天，且近30天无销售的鞋类商品)';
-- 服装30天不动销款数-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `clothe_stay_sku_cnt_similar` int COMMENT '服装30天不动销款数-同类值,同类店铺的(到店天数>30天，且近30天无销售的服装商品)';
-- 配件30天不动销款数-同类值
ALTER TABLE dm_similar_store_daily_stats_tmp ADD COLUMN `part_stay_sku_cnt_similar` int COMMENT '配件30天不动销款数-同类值,同类店铺的(到店天数>30天，且近30天无销售的配件商品)';

-- ---------------------------------------------------------------- 日 ---------------------------------------------------------------------------

-- ---------------------------------------------------------------- 建表 ---------------------------------------------------------------------------
CREATE TABLE `dm_similar_store_hour_stats_daily_tmp` (
                                                    `statis_tm` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                    `store_type_class` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '店铺类型分类',
                                                    `brand_name` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                    `hour_range` varchar(10) DEFAULT NULL COMMENT '小时数，当为ALL时，即为平均时段数据',
                                                    `customer_cnt_similar` decimal(10,4) DEFAULT NULL COMMENT '时段客流-同类值,周期内对应时段日均客流',
                                                    `sales_amt_actual_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额-同类值,周期内对应时段日均销售额',
                                                    `cr_similar` decimal(32,4) DEFAULT NULL COMMENT '时段成交率-同类值,同类店铺 时段日均小票数÷时段日均客流',
                                                    `customer_cnt_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段客流占比-同类值,同类店铺 时段日均客流÷日均客流',
                                                    `sales_amt_actual_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额占比-同类值,同类店铺 时段日均销售额÷日均销售额',
                                                    KEY `dm_similar_store_hour_stats_daily_tmp_tm` (`statis_tm`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

CREATE TABLE `dm_similar_store_hour_stats_weekly_tmp` (
                                                         `statis_tm` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                         `store_type_class` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '店铺类型分类',
                                                         `brand_name` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                         `hour_range` varchar(10) DEFAULT NULL COMMENT '小时数，当为ALL时，即为平均时段数据',
                                                         `customer_cnt_similar` decimal(10,4) DEFAULT NULL COMMENT '时段客流-同类值,周期内对应时段日均客流',
                                                         `sales_amt_actual_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额-同类值,周期内对应时段日均销售额',
                                                         `cr_similar` decimal(32,4) DEFAULT NULL COMMENT '时段成交率-同类值,同类店铺 时段日均小票数÷时段日均客流',
                                                         `customer_cnt_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段客流占比-同类值,同类店铺 时段日均客流÷日均客流',
                                                         `sales_amt_actual_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额占比-同类值,同类店铺 时段日均销售额÷日均销售额',
                                                         KEY `dm_similar_store_hour_stats_weekly_tmp_tm` (`statis_tm`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

CREATE TABLE `dm_similar_store_hour_stats_monthly_tmp` (
                                                         `statis_tm` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                         `store_type_class` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '店铺类型分类',
                                                         `brand_name` varchar(32) COLLATE utf8mb4_general_ci DEFAULT NULL,
                                                         `hour_range` varchar(10) DEFAULT NULL COMMENT '小时数，当为ALL时，即为平均时段数据',
                                                         `customer_cnt_similar` decimal(10,4) DEFAULT NULL COMMENT '时段客流-同类值,周期内对应时段日均客流',
                                                         `sales_amt_actual_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额-同类值,周期内对应时段日均销售额',
                                                         `cr_similar` decimal(32,4) DEFAULT NULL COMMENT '时段成交率-同类值,同类店铺 时段日均小票数÷时段日均客流',
                                                         `customer_cnt_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段客流占比-同类值,同类店铺 时段日均客流÷日均客流',
                                                         `sales_amt_actual_pct_similar` decimal(32,4) DEFAULT NULL COMMENT '时段销售额占比-同类值,同类店铺 时段日均销售额÷日均销售额',
                                                         KEY `dm_similar_store_hour_stats_monthly_tmp_tm` (`statis_tm`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;
